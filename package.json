{
  "name": "metatag",
  "description": "metatag is a Mutagen inspired library for parsing audio metadata formats like ID3.",
  "keywords": [
    "id3",
    "metadata",
    "audio",
    "mp3",
    "mutagen"
  ],
  "version": "0.4.3",
  "dependencies": {
    "sprintf-js": "~0.0.7",
    "iconv": "~2.1",
    "q": "~0.9",
    "minimist": "~0.1.0"
  },
  "devDependencies": {
    "coffee-script": "~1.7",
    "vows": "~0.7",
    "should": "~4.0",
    "rewire": "~2.0"
  },
  "main": "lib/metatag.js",
  "scripts": {
    "build": "coffee --bare --compile -o lib/ src/",
    "test": "vows --spec",
    "start": "node app.js",
    "prepublish": "npm run-script build",
    "pretest": "npm run-script build"
  },
  "bin": "./bin/metatag",
  "author": "John Guidry <lowdowndirtyrottenscoundrel@gmail.com>",
  "license": "GPL-2.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/dirtyrottenscoundrel/metatag.git"
  },
  "bugs": {
    "url": "http://github.com/dirtyrottenscoundrel/metatag/issues"
  }
}
